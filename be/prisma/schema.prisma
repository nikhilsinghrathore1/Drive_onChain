
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum status {
 inActive
 active
}

model User {
  id Int @id @default(autoincrement())
  firstName String 
  email String @unique
  lastName String?
  password String 
  socketId String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model BlackListToken{
  id Int @id @default(autoincrement())
  token String 
  createdAt DateTime @default(now())
}

model Captain { 
  id Int @id @default(autoincrement())
  fullName String
  email String @unique
  password String
  socketId String?
  status status @default(inActive)
  vehical_type String
  color String 
  plateNumber Int  
  capacity Int
  ltd Float?
  lng Float?
}